---
layout: null
permalink: /images.json
---

[
{% assign files = site.static_files | where_exp: "f", "f.path contains '/images/'" %}
{% assign exts = "jpg,jpeg,png,gif,webp,webm,mp4,mov" | split: "," %}
{% assign first = true %}
{% for f in files %}
  {% assign ext = f.extname | remove_first: "." | downcase %}
  {% if exts contains ext %}
    {% if first %}{% assign first = false %}{% else %},{% endif %}
    "{{ f.path | relative_url }}"
  {% endif %}
{% endfor %}
]
